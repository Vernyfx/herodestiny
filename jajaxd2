local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "VernyHub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


--locals

local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart

local spawns = game:GetService("Workspace").Spawns




--da thing

Tab:AddToggle({
	Name = "Auto Punch",
	Default = false,
	Callback = function(APValue)
		_G.autop = APValue

		task.spawn(function()
			while _G.autop do
		local args = {
			[1] = "Punch",
			[2] = "Left"
		}
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		wait(0.03)
		local args = {
			[1] = "Punch",
			[2] = "Right"
		}
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		wait(0.03)
			end
		end)
	end    
})

Tab:AddToggle({
	Name = "Auto Train",
	Default = false,
	Callback = function(ATValue)
		_G.autot = ATValue

		while _G.autot do
local args = {
    [1] = "Train"
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
wait(0.3)
		end
	end    
})






Tab:AddLabel("Auto Get Quest may glitch out and doesnt take it")

Tab:AddLabel("so sometimes u have to re-enable it")

local quests = {}
for i=1,36 do
      table.insert(quests,i)
end



Tab:AddDropdown({
	Name = "Select Quest",
	Default = " ",
	Options = quests,
	Callback = function(QValue)
		_G.quests = QValue
	end    
})




Tab:AddToggle({
	Name = "Auto Get Quest",
	Default = false,
	Callback = function(AQValue)
		_G.autoquest = AQValue

task.spawn(function()
    while _G.autoquest do
        wait(1)
        repeat 
            local args = {
                [1] = "GetQuest",
                [2] = _G.quests
            }    
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            wait(1)
        until game:GetService("Players").LocalPlayer:WaitForChild("Quest")
    end
end)
	end    
})





local mobs = game:GetService("Workspace").Spawns:GetChildren()

local mobsTable = {}

for i,v in ipairs(mobs) do
    if v:IsA("Part") then
        table.insert(mobsTable, v.Name)
    end
end

Tab:AddDropdown({
	Name = "Select Mob",
	Default = " ",
	Options = mobsTable,
	Callback = function(SMValue)
		_G.selectmobs = SMValue
	end    
})

Tab:AddLabel("Make Sure to enable auto punch")

Tab:AddToggle({
	Name = "Auto Tp To Mob",
	Default = false,
	Callback = function(ATMValue)
		_G.automob = ATMValue;

        task.spawn(function()
            while _G.automob do
                if _G.selectmobs ~= nil then
                    wait(0.05)
                    hrp.CFrame = game:GetService("Workspace").Spawns:WaitForChild(_G.selectmobs):WaitForChild(_G.selectmobs).HumanoidRootPart.CFrame * CFrame.new(0,0,5)
                    wait(0.05)
                end
            end
        end)
	end    
})

local Tab = Window:MakeTab({
	Name = "Spin",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})





Tab:AddToggle({
	Name = "Auto Buy Spins",
	Default = false,
	Callback = function(ABSValue)
		_G.autobuyspin = ABSValue

		task.spawn(function()
	while _G.autobuyspin do
local args = {
    [1] = "PurchaseSpin"
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
wait(0.01)
	end
end)
	end    
})

Tab:AddDropdown({
	Name = "Select Class 1",
	Default = " ",
	Options = {"Demon", "Arcane Knight", "Toxin", "G.O.D", "Thor", "Ultrahuman", "Hero Hunter", "Blast", "Gravity", "Bing Bong", "Cosmic", "Reaper" },
	Callback = function(CValue)
		_G.class = CValue;
	end    
})

Tab:AddDropdown({
	Name = "Select Class 2",
	Default = " ",
	Options = {"Demon", "Arcane Knight", "Toxin", "G.O.D", "Thor", "Ultrahuman", "Hero Hunter", "Blast", "Gravity", "Bing Bong", "Cosmic", "Reaper" },
	Callback = function(CValue)
		_G.class2 = CValue;
	end    
})

Tab:AddDropdown({
	Name = "Select Class 3",
	Default = " ",
	Options = {"Demon", "Arcane Knight", "Toxin", "G.O.D", "Thor", "Ultrahuman", "Hero Hunter", "Blast", "Gravity", "Bing Bong", "Cosmic", "Reaper" },
	Callback = function(CValue)
		_G.class3 = CValue;
	end    
})

Tab:AddDropdown({
	Name = "Select Class 4",
	Default = " ",
	Options = {"Demon", "Arcane Knight", "Toxin", "G.O.D", "Thor", "Ultrahuman", "Hero Hunter", "Blast", "Gravity", "Bing Bong", "Cosmic", "Reaper" },
	Callback = function(CValue)
		_G.class4 = CValue;
	end    
})


Tab:AddToggle({
	Name = "Auto Spin",
	Default = false,
	Callback = function(ASValue)
		_G.autospin = ASValue;
	end
})

task.spawn(function()
	while wait() do
		if _G.autospin and game:GetService("Players").LocalPlayer.Stats.Class.Value ~= _G.class and _G.class2 and _G.class3 and _G.class4 then
			wait(0.01)
			game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer()
			wait(0.1)
		end
	end
end)

local Tab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



Tab:AddToggle({
	Name = "Auto Put Stamina",
	Default = false,
	Callback = function(APSValue)
		_G.autostam = APSValue
	end    
})

task.spawn(function()
	while wait() do
		if _G.autostam then
			local args = {
    [1] = "UpgradeStamina",
    [2] = 1
}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		end
	end
end)



Tab:AddToggle({
	Name = "Auto Put Health",
	Default = false,
	Callback = function(APHValue)
		_G.autohp = APHValue
	end    
})

task.spawn(function()
	while wait() do
		if _G.autohp then
local args = {
    [1] = "UpgradeHealth",
    [2] = 1
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		end
	end
end)

